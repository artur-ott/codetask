# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index
GET     /login                      controllers.Auth.login
GET     /logout                     controllers.Auth.logout
GET     /register                   controllers.Auth.register
POST    /submit                     controllers.Auth.submit
POST    /authenticate               controllers.Auth.authenticate

GET     /courses                    controllers.Application.courses
GET     /course/add/:course         controllers.Application.addCourse(course: String)
GET     /course/remove/:course      controllers.Application.removeCourse(course: String)

GET     /dashboard                  controllers.Application.dashboard

GET     /test                       controllers.Application.test
GET     /securteTest                controllers.Application.secureTest
GET     /testInterpret/:code        controllers.Application.testInterpret(code: String)
GET     /testSocket                 controllers.Application.socket
#POST    /interpret                 controllers.Application.interpret

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
