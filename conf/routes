# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /login                      controllers.Auth.login
GET     /authenticate               controllers.Auth.authenticate
GET     /index                      controllers.Application.index
GET     /testInterpret/:code        controllers.Application.testInterpret(code: String)
GET     /test                       controllers.Application.test
GET     /testSocket                 controllers.Application.socket
#POST    /interpret                 controllers.Application.interpret

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
